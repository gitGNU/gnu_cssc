dir=${dir:-../..}
get=${get:-${dir}/get}
admin=${admin:-${dir}/admin}
cdc=${cdc:-${dir}/cdc}
prs=${prs:-${dir}/prs}
prt=${prt:-${dir}/prt}
delta=${delta:-${dir}/delta}
sact=${sact:-${dir}/sact}
sccsdiff=${sccsdiff:-${dir}/sccsdiff}
unget=${unget:-${dir}/unget}
what=${what:-${dir}/what}
rmdel=${rmdel:-${dir}/rmdel}

if test -f ${dir}/sccs
then
	sccs=${dir}/sccs
else
	case ${dir} in 
		../..) sccs=${dir}/bsd/sccs ;;
		    *) sccs=sccs            ;;
	esac
fi


DIFF=${DIFF:-diff}
